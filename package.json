{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "real": "npm run format && npm run clean && npm run build  && npm run serve",
    "serve": "npm run format && gatsby serve --host=0.0.0.0 -p 9000",
    "github": "npm run format && npm run clean && npm run build && gh-pages -d public"
  },
  "dependencies": {
    "gatsby": "^2.24.67",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-scroll-reveal": "0.0.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
